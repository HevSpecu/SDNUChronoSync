---
import DashboardLayout from '../../layouts/DashboardLayout.astro';
import MySchedulePage from '../../components/MySchedulePage.vue';
---

<DashboardLayout title="我的课表 - 课表管理系统">
  <div class="space-y-6">
    <div class="sm:flex sm:items-center">
      <div class="sm:flex-auto">
        <h1 class="text-2xl font-semibold text-gray-900">我的课表</h1>
        <p class="mt-2 text-sm text-gray-700">
          管理您的个人课程和日程安排
        </p>
      </div>
    </div>

    <MySchedulePage client:load />
  </div>
</DashboardLayout>
